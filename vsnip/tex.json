{
  "Environment": {
    "prefix": ["en"],
    "body": ["\\begin{${1}}", 
                "${2}",
                "\\end{$1}",
                "$0"
    ],
    "description": "LaTex Environment."
  },
  "Equation": {
    "prefix": ["eq"],
    "body": ["\\begin{${1|equation,equation*|}}", 
                "\\begin{aligned}",
                "${2}",
                "\\end{aligned}",
                "\\end{$1}",
                "$0"
    ],
    "description": "LaTex Equation."
  },
  "Inside Equation": {
    "prefix": ["ie"],
    "body": ["${1} &${2} \\\\\\\\ $0"],
    "description": "LaTex Inside Equation."
  },
  "Group Theory Sum": {
    "prefix": ["gsum"],
    "body": ["\\sum_{${1} \\in ${2}} $0"],
    "description": "LaTex Group Theory Sum."
  },
  "Sum": {
    "prefix": ["sum"],
    "body": ["\\sum_{${1} = ${2}}^{${3}} $0"],
    "description": "LaTex Sum."
  },
  "Product": {
    "prefix": ["pr"],
    "body": ["\\prod_{${1} = ${2}}^{${3}} $0"],
    "description": "LaTex Product."
  },
  "Union": {
    "prefix": ["cu"],
    "body": ["\\cup_{${1} = ${2}}^{${3}} $0"],
    "description": "LaTex Cup."
  },
  "Intersection": {
    "prefix": ["ca"],
    "body": ["\\cap{${1} = ${2}}^{${3}} $0"],
    "description": "LaTex Cap."
  },
  "Ceiling Function": {
    "prefix": ["ce", "ceil"],
    "body": ["\\left\\lceil ${1} \\right\\rceil $0"],
    "description": "LaTeX Ceiling."
  },
  "Floor Function": {
    "prefix": ["flr", "floor"],
    "body": ["\\left\\lfloor ${1} \\right\\rfloor $0"],
    "description": "LaTeX Floor."
  },
  "Absolute Value": {
    "prefix": ["abs"],
    "body": ["\\lvert ${1} \\rvert $0"],
    "description": "LaTeX Absolute Value."
  },
  "Exponent": {
    "prefix": ["e"],
    "body": ["^{${1}} $0"],
    "description": "LaTeX Exponent."
  },
  "Congruent": {
    "prefix": ["cmod"],
    "body": ["\\equiv ${1} \\pmod{${2}} $0"],
    "description": "LaTeX congruence."
  },
  "Subscript": {
    "prefix": ["su"],
    "body": ["${1}_{${2}} $0"],
    "description": "LaTeX Subscript."
  },
  "Fraction": {
    "prefix": ["fr"],
    "body": ["\\frac{${1}}{${2}} $0"],
    "description": "LaTeX Fraction."
  },
  "binomial": {
    "prefix": ["bin"],
    "body": ["\\binom{${1}}{${2}} $0"],
    "description": "LaTeX binomial."
  },
  "Inner Product": {
    "prefix": ["ip"],
    "body": ["\\left\\langle ${1} \\right\\rangle$0"],
    "description": "LaTeX inner product."
  },
  "Bracket": {
    "prefix": ["br"],
    "body": ["\\left(${1}\\right) $0"],
    "description": "LaTeX Bracket."
  },
  "Bracket Rectangular": {
    "prefix": ["rbr"],
    "body": ["\\left[${1}\\right] $0"],
    "description": "LaTeX Bracket Rectangular."
  },
  "Bracket Squiqqly": {
    "prefix": ["sbr"],
    "body": ["\\left\\{${1}\\right\\\\} $0"],
    "description": "LaTeX Bracket Rectangular."
  },
  "Norm of vector": {
    "prefix": ["nor"],
    "body": ["\\left\\lVert ${1}\\right\\rVert $0"],
    "description": "LaTeX Norm."
  },
  "Text Normal": {
    "prefix": ["tn"],
    "body": ["\\textnormal{${1}} $0"],
    "description": "LaTeX Text Normal."
  },
  "Text Bold": {
    "prefix": ["tb"],
    "body": ["\\textbf{${1}} $0"],
    "description": "LaTeX Text Bold."
  },
  "Text Italicized": {
    "prefix": ["ti"],
    "body": ["\\textit{${1}} $0"],
    "description": "LaTeX Text Italic."
  },
  "Text Coloured": {
    "prefix": ["tc"],
    "body": ["\\textcolor{${1:col}}{${2:text}} $0"],
    "description": "LaTeX Text Colour."
  },
  "Text Spoken": {
    "prefix": ["ts"],
    "body": ["[\\textcolor{WildStrawberry}{Spoken:${1}}] $0"]
  },
  "Text Discussion": {
    "prefix": ["td"],
    "body": ["[\\textcolor{BlueViolet}{Spoken:${1}}] $0"]
  },
  "Text Underlined": {
    "prefix": ["tu"],
    "body": ["\\underline{${1}} $0"],
    "description": "LaTeX Text Underlined."
  },
  "Mathcal Text": {
    "prefix": ["mc"],
    "body": ["\\mathcal{${1}}$0"],
    "description": "LaTeX Mathcal."
  },
  "Mathbb Text": {
    "prefix": ["mb"],
    "body": ["\\mathbb{${1}}$0"],
    "description": "LaTeX Mathbb."
  },
  "Graphic": {
    "prefix": ["incgrah"],
    "body": ["\\includegraphics[scale = ${1:scale}]{${2:path}} $0"],
    "description": "LaTeX graphic."
  },
  "TikzPicture": {
    "prefix": ["tikz"],
    "body": ["\\begin{tikzpicture}", 
                "$0",
                "\\end{tikzpicture}"
    ],
    "description": "LaTeX TikzPicture."
  },
  "Tikz labelled node": {
    "prefix": ["lnode"],
    "body": ["\\node[${1:options}] (${2:label}) at (${3:placement}) {$2};",
	    "$0"
    ],
    "description": "LaTeX TikzPicture labelled node."
  },
  "Tikz node": {
    "prefix": ["node"],
    "body": ["\\node[${1:options}] at (${3:placement}) {};",
	    "$0"
    ],
    "description": "LaTeX TikzPicture node."
  },
  "Tikz edge": {
    "prefix": ["edge"],
    "body": ["\\path[${1:options}] (${2:placement}) edge (${3:placement});",
	    "$0"
    ],
    "description": "LaTeX TikzPicture edge."
  }
}
