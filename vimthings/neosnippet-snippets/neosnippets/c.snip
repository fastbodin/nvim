snippet     ifelse
options     head
abbr        if () {} else {}
    if (${1:#:condition}) {
        ${2:TARGET}
    } else {
        ${3}
    }

snippet equn
abbr "Equation"
    \begin{equation}
            ${1} &= ${2} \\\\ ${3}
    \end{equation}
    ${4}

snippet br
abbr   "( ) - bracket"
    \left( ${1} \right) ${2}

snippet frac
abbr "Fraction"
    \frac{${1}}{${2}} ${3}
